generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Feedback {
  id              Int              @id @default(autoincrement())
  createdAt       DateTime         @default(now())
  email           String?
  questions       String
  domains         String
  name            String?
  ip              String?
  geoCity         String?
  geoCountry      String?
  geoRegion       String?
  geoLatitude     String?
  geoLongitude    String?
  deviceModel     String?
  deviceVendor    String?
  deviceType      String?
  browserName     String?
  browserVersion  String?
  osName          String?
  osVersion       String?
  engineName      String?
  engineVersion   String?
  isBot           Boolean          @default(false)
  userAgent       String?
  origin          String?
  referer         String?
}